{
  "name": "rextore",
  "version": "1.0.0",
  "description": "Reactive state container based in RxJs and inspired by Redux",
  "author": {
    "name": "Pablo Magaz",
    "email": "magazpablo@gmail.com"
  },
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist coverage",
    "build": "npm run clean && tsc",
    "lint": "tslint \"src/**/*.ts\"",
    "test:coverage": "nyc npm test && nyc report --reporter=lcov",
    "test": "mocha -r ts-node/register **/*.spec.ts",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "prepublish": "npm run lint && npm run test:coverage && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmagaz/rextore.git"
  },
  "keywords": [
    "reactive",
    "redux",
    "store",
    "state",
    "container",
    "rxjs",
    "rextore",
    "rxjs-redux",
    "stream",
    "observable"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pmagaz/rextore/issues"
  },
  "homepage": "https://github.com/pmagaz/rextore#readme",
  "dependencies": {
    "rxjs": "5.5.6"
  },
  "devDependencies": {
    "@types/mocha": "2.2.48",
    "@types/node": "9.4.6",
    "chai": "4.1.2",
    "coveralls": "3.0.0",
    "mocha": "5.0.1",
    "nyc": "11.4.1",
    "rimraf": "2.6.2",
    "ts-node": "5.0.0",
    "tslint": "5.9.1",
    "tslint-config-prettier": "1.8.0",
    "tslint-eslint-rules": "5.0.0",
    "typescript": "2.7.2"
  }
}
